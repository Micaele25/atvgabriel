public class carro {
    String marca;
    String modelo;
    String cor;
    int ano;
    int numero_portas;

	public carro(){}

	public carro (String marca,String modelo,String cor,int ano,int numero_portas) {
this.marca = marca;
this.modelo = modelo;
this.cor = cor;
this.ano = ano;
this.numero_portas = numero_portas;

}


public String descricao()
        return "Marca: " + marca + ", Modelo: " + modelo + ", Ano: " + ano + ", Cor: " + cor + ", Numero_portas: " + numero_portas;
	}
}
